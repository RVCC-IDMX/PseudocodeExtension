// ----------------------------------------
// SECTION 1: CONTROL FLOW KEYWORDS
// ----------------------------------------

BEGIN
    // Test all control flow keywords
END

IF condition THEN
    // IF block
ENDIF

IF condition THEN
    // True branch
ELSE
    // False branch
ENDIF

IF condition1 THEN
    // First condition
ELSEIF condition2 THEN
    // Second condition
ELSE
    // Default
ENDIF

// ----------------------------------------
// SECTION 2: LOOP KEYWORDS
// ----------------------------------------

FOR i = 1 TO 10 STEP 1
    // FOR loop content
NEXT i

// Alternative FOR terminator
FOR j = 1 TO 5 STEP 2
    // Loop body
ENDFOR

// Another FOR terminator
FOR k = 10 TO 1 STEP -1
    // Loop body
END FOR

WHILE condition
    // WHILE loop content
ENDWHILE

REPEAT
    // REPEAT loop content
UNTIL condition

// ----------------------------------------
// SECTION 3: FUNCTION KEYWORDS
// ----------------------------------------

FUNCTION myFunction ()
BEGIN
    // Function body
END

FUNCTION functionWithParams (param1, param2)
BEGIN
    RETURN result
END FUNCTION

// Function calls
CALL myFunction()
CALL functionWithParams(arg1, arg2)

// Exit function early
EXIT

