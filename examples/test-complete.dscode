SET IFFY = true
SET SETUP = "configuration"

// Comment symbols in strings (should NOT create comments)
SET comment1 = "This is not // a comment"
SET comment2 = "This is not /* a comment */ either"

// Numbers in various contexts
SET id = 12345
SET mixed = "ID: " + 12345
SET calculation = 100 + 50 * 2

// ----------------------------------------
// SECTION 16: NESTED STRUCTURES
// ----------------------------------------

FUNCTION complexFunction (x, y)
BEGIN
    IF x > 0 THEN
        WHILE y > 0
            FOR i = 1 TO x STEP 1
                IF i = y THEN
                    RETURN i * y
                ENDIF
            NEXT i
            SET y = y - 1
        ENDWHILE
    ELSE
        REPEAT
            SET x = x + 1
        UNTIL x > 0
    ENDIF
    RETURN 0
END

